{
  "kind": "collectionType",
  "collectionName": "housings",
  "info": {
    "singularName": "housing",
    "pluralName": "housings",
    "displayName": "housing",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "maxLength": 70
    },
    "caption": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true,
      "maxLength": 300
    },
    "getMoneyWays": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "sell",
        "monthly rent",
        "daily rent"
      ]
    },
    "cost": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "decimal"
    },
    "housingType": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "apartment",
        "complex",
        "villa"
      ],
      "required": true
    },
    "city": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    },
    "district": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    },
    "housingArea": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "float"
    },
    "adjacentArea": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "float"
    },
    "locationUrl": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "regex": "([h][t]{2}[p][s][:][\\/]{2}[g][o]{2}[\\.][g][l][\\/][maps]{4}[\\/]|[h][t]{2}[p][s][:][\\/]{2}[y][a][n][d][e][x][\\.][r][u][\\/][m][a][p][s][\\/]|[h][t]{2}[p][s][:][\\/]{2}[g][o][\\.][2][g][i][s][\\.][c][o][m][\\/])"
    },
    "agent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::agent.agent",
      "inversedBy": "housings"
    },
    "favoriteUsers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::telegram-user.telegram-user",
      "inversedBy": "favoriteHousings"
    },
    "watchedUsers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::telegram-user.telegram-user",
      "inversedBy": "watchedHousings"
    },
    "rooms": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "housings"
    },
    "constructionCompletionDate": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "date"
    },
    "metersFromTheSea": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer"
    },
    "layoutPhoto": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
